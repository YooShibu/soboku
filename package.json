{
  "name": "soboku",
  "version": "0.1.0",
  "description": "managing small state",
  "main": "dist/soboku.js",
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.14",
    "istanbul": "^0.4.5",
    "jasmine": "^2.6.0",
    "remap-istanbul": "^0.9.5",
    "rollup": "^0.43.0",
    "rollup-plugin-typescript": "^0.8.1",
    "typescript": "^2.4.1"
  },
  "scripts": {
    "pretest": "tsc",
    "test": "istanbul cover ./node_modules/jasmine/bin/jasmine.js -- --config=./jasmine.json",
    "posttest": "npm run remap-coverage-html && npm run remap-coverage-json",
    "prepublish": "node ./bundle.js",
    "bundle": "node ./bundle.js",
    "remap-coverage-html": "remap-istanbul -i ./coverage/coverage.json -o ./coverage/ts-report -t html",
    "remap-coverage-json": "remap-istanbul -i ./coverage/coverage.json -o ./coverage/lconv.ts.json -t json"
  },
  "keywords": [
    "state"
  ],
  "author": "Yohei Shibuya",
  "license": "MIT",
  "files": [
    "dist",
    "index.d.ts",
    "LISENCE",
    "package.json",
    "README.md"
  ],
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YooShibu/soboku.git"
  },
  "bugs": {
    "url": "https://github.com/YooShibu/soboku/issues"
  },
  "homepage": "https://github.com/YooShibu/soboku#readme"
}
